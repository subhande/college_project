<div>

	<div id=coming class="container"><h3>Upcoming Events</h3>
		<hr>
		{{#each events}}
			<div class="card">
				<div class="card-header">
					<a href="/event/register">
						<div class="row">
							<div class="col-9">{{name}}</div>
							<div class="col-3">{{formatDate date}}</div>
						</div>
					</a>
				</div>
			</div>
		{{/each}}
	</div>

</div>