<h1>Subjects</h1>


<table class="table table-bordered table-responsives">
	<thead>
	<tr>
		<th>Subject Name</th>
		<th>Subject Code</th>
		<th>Remove</th>
	</tr>
	</thead>

	<tbody>
	{{#each subs}}
		<tr>
			<th>{{name}}</th>
			<th>{{code}}</th>
			<th>
				<form action="/faculty/subjects/{{_id}}?_method=PATCH" method="post">
					<button class="btn btn-danger" type="submit" >Remove</button>
				</form>
			</th>
		</tr>
	{{/each}}
	</tbody>
</table>