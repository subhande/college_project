<h1>Welcome to Student Section</h1>

<table class="table table-info table-bordered">
	<tr>
		<th>Name</th>
		<td>{{name}}</td>
	</tr>
	<tr>
		<th>Username</th>
		<td>{{username}}</td>
	</tr>
	<tr>
		<th>Email</th>
		<td>{{email}}</td>
	</tr>
	<tr>
		<th>Course</th>
		<td>{{course}}</td>
	</tr>
	<tr>
		<th>Branch</th>
		<td>{{branch}}</td>
	</tr>
	<tr>
		<th>Semester</th>
		<td>{{sem}}</td>
	</tr>
	<tr>
		<th>Roll No</th>
		<td>{{rollno}}</td>
