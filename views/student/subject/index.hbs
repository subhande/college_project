<h1>Enrolled Subjects</h1>


<table class=" table table-bordered table-responsives">
	<thead>
		<tr>
			<th>Subject Name</th>
			<th>Subject Code</th>
			<th>Faculty Name</th>
			<th>Action</th>
		</tr>
	</thead>

	<tbody>
		{{#each subs}}
			<tr>
				<th>{{name}}</th>
				<th>{{code}}</th>
				<th>{{faculty.user.firstName}} {{faculty.user.lastName}}</th>
				<th>
					<form action="/student/subject/{{_id}}?_method=PATCH" METHOD="POST">
						<button class="btn btn-danger">Unenroll</button>
					</form>
				</th>
			</tr>
		{{/each}}
	</tbody>


</table>

