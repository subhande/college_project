<h1>Enrolled Subjects</h1>


<table class=" table table-bordered">
	<thead>
	<tr>
		<th>Subject Name</th>
		<th>Subject Code</th>
		<th>Faculty Name</th>
		<th>Action</th>
	</tr>
	</thead>
	<tbody>
		{{#each subs}}
			{{!--{{#if true}}--}}
				<tr>
					{{!--<th>{{isEnrolled _id sid}}</th>--}}
					<th>{{name}}</th>
					<th>{{code}}</th>
					<th>{{faculty.user.firstName}} {{faculty.user.lastName}}</th>
					<th>
						<form action="/student/subject/add/{{id}}" method="post">
							<button class="btn btn-primary" type="submit">Add</button>
						</form>
					</th>
				</tr>
			{{!--{{/if}}--}}
		{{/each}}
	</tbody>


</table>

